{
  "devDependencies": {
    "add": "^2.0.6",
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "browserify": "^13.3.0",
    "faucet": "^0.0.1",
    "flow": "^0.2.3",
    "flow-bin": "^0.37.4",
    "meow": "^3.7.0",
    "nexe": "https://github.com/nexe/nexe.git",
    "tape": "^4.6.3",
    "tape-catch": "^1.0.6"
  },
  "dependencies": {
    "minimist": "^1.2.0",
    "ohm-js": "^0.12.3"
  },
  "scripts": {
    "nao": "node lib/cli.js",
    "one": "mkdir -p dist/ && nexe -i $PWD/lib/cli.js -p $(which python2.7) -o $PWD/dist/nao^\\$",
    "watch": "babel --watch=src --out-dir=./lib --copy-files"
  },
  "bin": {
    "nao": "lib/cli.js"
  },
  "files": [
    "lib"
  ],
  "babel": {
    "plugins": [
      "transform-flow-strip-types"
    ]
  }
}
